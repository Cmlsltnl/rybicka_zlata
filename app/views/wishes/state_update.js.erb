$(".<%= anchor_for(@wish) %>").each(function(){ 
  $(this).replaceWith("<%= escape_javascript( render(  partial: "/shared/list_with_actions_item", locals: { object: @wish, remote: true}) ) %>");
  $(this).effect("highlight", 500);
 }); 

document.querySelector('#snackbar').MaterialSnackbar.showSnackbar(
  {
    message: "<%= raw flash[:notice] %>"
  }
);

