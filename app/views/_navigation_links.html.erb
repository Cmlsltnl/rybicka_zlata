<% if user_signed_in? %>
  <%= link_to(t("wish.from_donor.views.index.header"), user_others_wishes_path(current_user), class: "#{link_class} is-active") %>
  <%= link_to(t("wish.from_donee.views.index.header"), user_my_wishes_path(current_user), class: link_class) %>
  <%= link_to(t("connection.views.header"), user_connections_path(current_user), class: link_class) %>
  <%= link_to(t("group.views.header"), user_groups_path(current_user), class: link_class) %>
  <%= link_to(t("user.profile.my_page.header"), my_page_path(), class: link_class) %>
  <%= link_to t("devise.sign_out"), destroy_user_session_path, :method => :delete, class: link_class %>
<% else %>
  <%= link_to t("devise.sign_up"), new_user_registration_path, class: link_class %>
  <%= link_to t("devise.sign_in"), new_user_session_path, class: link_class %>
  <%= link_to t("rybickazlata.static.about"), "#static_about", class: link_class %>
<% end %>
 

 
